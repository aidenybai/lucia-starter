{
  "name": "create-lucia-starter",
  "version": "0.0.1",
  "bin": "bin/setup",
  "scripts": {
    "dev": "npm run reset && parcel src/index.html --out-dir development -p 3000",
    "build": "npm run reset && parcel build src/*.html --out-dir dist --public-url ./",
    "reset": "rimraf ./development && rimraf -rf ./.cache && rimraf ./dist",
    "cleanup": "prettier -w src"
  },
  "dependencies": {
    "lucia": "^0.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime-corejs2": "^7.9.2",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
