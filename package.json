{
  "name": "lucia-starter",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run clean && parcel src/index.html --out-dir development -p 3000",
    "build": "parcel build src/*.html --out-dir dist --public-url ./",
    "reset": "rimraf ./development && rimraf -rf ./.cache",
    "cleanup": "prettier -w src"
  },
  "dependencies": {
    "lucia": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2"
  }
}
